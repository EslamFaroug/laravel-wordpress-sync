{
  "name": "eslamfaroug/laravel-wordpress-sync",
  "description": "A Laravel package to sync articles with WordPress via API based on language.",
  "type": "library",
  "autoload": {
    "psr-4": {
      "EslamFaroug\\LaravelWordpressSync\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "guzzlehttp/guzzle": "^7.0"
  },
  "authors": [
    {
      "name": "Eslam Faroug",
      "email": "eslamfaroug3@gmail.com",
      "homepage": "https://github.com/eslamfaroug",
      "role": "Developer"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/eslamfaroug/laravel-wordpress-sync.git"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "EslamFaroug\\LaravelWordpressSync\\LaravelWordpressSyncServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true
}
